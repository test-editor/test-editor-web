{
  "name": "test-editor-web",
  "version": "1.1.6",
  "license": "EPL",
  "scripts": {
    "ng": "node_modules/@angular/cli/bin/ng",
    "upgradeTesteditorOrgDependencies": "yarn upgrade -S @testeditor",
    "prestart": "yarn run upgradeTesteditorOrgDependencies",
    "start": "ng serve --host 0.0.0.0",
    "prebuild": "yarn run upgradeTesteditorOrgDependencies && ng lint",
    "build": "ng build",
    "bareBuild": "ng build",
    "pretest": "yarn run upgradeTesteditorOrgDependencies",
    "test": "ng test",
    "test:once": "ng test --watch=false",
    "pretestCIChrome": "yarn run upgradeTesteditorOrgDependencies",
    "testCIChrome": "ng test --browser ChromeHeadless --watch=false --log-level debug",
    "pretestCIFirefox": "yarn run upgradeTesteditorOrgDependencies",
    "testCIFirefox": "ng test --browser Firefox --watch=false --log-level debug",
    "lint": "ng lint",
    "pree2e": "webdriver-manager update --ignore_ssl --gecko false --standalone false",
    "e2e": "ng e2e --webdriver-update=false",
    "docker:build": "docker build -t testeditor/web:snapshot .",
    "docker:run": "docker run -p 4200:4200 testeditor/web:snapshot &",
    "postversion": "git push && git push --tags"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.10",
    "@angular/common": "^7.2.10",
    "@angular/compiler": "^7.2.10",
    "@angular/core": "^7.2.10",
    "@angular/forms": "^7.2.10",
    "@angular/http": "^7.2.10",
    "@angular/platform-browser": "^7.2.10",
    "@angular/platform-browser-dynamic": "^7.2.10",
    "@angular/router": "^7.2.10",
    "@auth0/angular-jwt": "^2.1.0",
    "@testeditor/messaging-service": "~1.5.0",
    "@testeditor/test-navigator": "~0.2.0",
    "@testeditor/testeditor-commons": "~0.6.0",
    "@testeditor/testexec-details": "~0.4.0",
    "@testeditor/testexec-navigator": "~0.5.0",
    "@testeditor/teststep-selector": "~0.2.0",
    "@testeditor/user-activity": "~0.1.0",
    "ace-builds": "^1.4.3",
    "angular-auth-oidc-client": "^9.0.3",
    "angular-split": "2.0.1",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.5",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "ng4-loading-spinner": "~1.2.0",
    "ngx-bootstrap": "^2.0.4",
    "popper.js": "^1.14.7",
    "prophecy": "^0.0.4",
    "requirejs": "^2.3.3",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.0.0-rc.0",
    "ts-clipboard": "^1.0.17",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "^7.3.6",
    "@angular/compiler-cli": "^7.2.10",
    "@angular/language-service": "^7.2.10",
    "@types/jasmine": "3.3.12",
    "@types/node": "^11.11.4",
    "codelyzer": "~5.0.0-beta.1",
    "jasmine-core": "^3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.0.0",
    "karma-junit-reporter": "^1.2.0",
    "protractor": "~5.4.2",
    "ts-mockito": "^2.3.0",
    "ts-node": "~8.0.3",
    "tslint": "5.11.0",
    "typescript": "~3.2.4"
  }
}
